{
    "name": "ipu-assembly-support",
    "displayName": "IPU Assembly Support",
    "description": "Code-completion and syntax highlighting for IPU assembly files.",
    "repository": "https://phabricator.sourcevertex.net/diffusion/IPUASSEMBLYSUPPORT/",
    "author": "Tim Hutt",
    "version": "2.0.0",
    "publisher": "Graphcore",
    "license": "MIT",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ipu_asm",
                "aliases": [
                    "IPU Assembly"
                ],
                "extensions": [
                    ".S",
                    ".s"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ipu_asm",
                "scopeName": "source.ipu_asm",
                "path": "./syntaxes/ipu_asm.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.15",
        "@types/vscode": "^1.64.0",
        "@typescript-eslint/parser": "^5.10.2",
        "esbuild": "^0.14.20",
        "eslint": "^8.8.0",
        "tree-sitter-cli": "^0.20.4",
        "typescript": "^4.5.5",
        "vsce": "^2.6.7",
        "vscode-test": "^1.6.1"
    },
    "main": "dist/extension.js",
    "activationEvents": [
        "onLanguage:ipu_asm"
    ]
}
